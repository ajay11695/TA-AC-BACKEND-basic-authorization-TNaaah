<%- include('partial/header') -%>
<main class="container1">
    <form action="/users/login" method="POST" class="form">
        <h1 class="text-align font-20-light f-weight margin-b-20">Log In</h1>
        <% if(error){ %>
            <p class=""><%= error %></p>
        <% } %>
        <input class="formInput margin-b-20" type="email" name="email" placeholder="Enter email...">
        <input class="formInput margin-b-20" type="password" name="password" placeholder="Enter password...">
        <div class="text-align">
            <input class="btn" type="submit" name="" id="" value="Log in">
        </div>
    </form>
</main>

<%- include('partial/footer') -%>